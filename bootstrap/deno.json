{
  "imports": {
    "@std/fs": "jsr:@std/fs@^1",
    "@std/log": "jsr:@std/log@^0.224",
    "@std/path": "jsr:@std/path@^1",
    "@std/cli": "jsr:@std/cli@^1",
    "@lttr/deno-dsc": "jsr:@lttr/deno-dsc@^0.1.0"
  },
  "tasks": {
    "run": "deno run --quiet --allow-read --allow-write --allow-run --allow-env --allow-net install.ts",
    "withreload": "deno run --reload --allow-read --allow-write --allow-run --allow-env --allow-net install.ts",
    "dryrun": "deno run --quiet --allow-read --allow-write --allow-run --allow-env --allow-net dry-run.ts",
    "print": "deno run --quiet --allow-read --allow-write --allow-env --allow-net print.ts",
    "stats": "deno run --quiet --allow-read --allow-write --allow-env stats.ts",
    "depgraph": "deno run --quiet --allow-read --allow-write --allow-run --allow-env dep-graph.ts",
    "list": "deno run -A list-resources.ts"
  }
}
