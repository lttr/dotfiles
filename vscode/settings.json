    "<C-]>": false,
    "<a-j>": false,
    "<a-k>": false,
    "<a-h>": false,
    "<a-l>": false,
    "tab": false
  },
  "vim.leader": " ",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "k"],
      "after": ["k"]
    },
    {
      "before": ["g", "j"],
      "after": ["j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": [" ", ";"],
      "after": ["m", "a", "A", ";", "<Esc>", "`", "a"]
    },
    {
      "before": [" ", ","],
      "after": ["m", "a", "A", ",", "<Esc>", "`", "a"]
    },
    {
      "before": ["g", "\""],
      "after": ["m", "a", "F", "'", "r", "\"", "f", "'", "r", "\"", "`", "a"]
    },
    {
      "before": ["g", "'"],
      "after": ["m", "a", "F", "\"", "r", "'", "f", "\"", "r", "'", "`", "a"]
    },
    {
      "before": ["g", "`"],
      "after": ["m", "a", "F", "'", "r", "`", "f", "'", "r", "`", "`", "a"]
    },
    {
      "before": ["leader", "\""],
      "after": ["m", "a", "b", "i", "\"", "<Esc>", "e", "a", "\"", "<Esc>", "`", "a"]
    },
    {
      "before": ["leader", "'"],
      "after": ["m", "a", "b", "i", "'", "<Esc>", "e", "a", "'", "<Esc>", "`", "a"]
    },
    {
      "before": ["leader", "`"],
      "after": ["m", "a", "b", "i", "`", "<Esc>", "e", "a", "`", "<Esc>", "`", "a"]
    },
    {
      "before": ["g", "m"],
      "after": ["<Esc>"],
      "commands": [
        {
          "command": "editor.action.changeAll",
          "args": []
        }
      ]
    },
    {
      "before": ["leader", "h"],
      "after": ["<Esc>"],
      "commands": [
        {
          "command": ":nohl",
          "args": []
        }
      ]
    },
    {
      "before": ["g", "S"],
      "after": ["\"", "d", "c", "i", "t", "<CR>", "a", "<Esc>", "\"", "d", "p", "^", "x"]
    }
  ],
